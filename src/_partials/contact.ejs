<section class="c-contact">
    <div class="l-container l-container--w1200">
        <div class="c-contact__contain">
            <h2 class="c-title">クリニック概要</h2>
        <div class="c-contact__main">
            <div class="c-contact__wrapimg">
                <img src="./assets/img/contact.jpg" alt="クリニック概要" class="c-contact__img">
            </div>
            <div class="c-contact__info">
                <table class="c-contact__table">
                    <tbody>
                        <tr class="c-contact__tr">
                            <th class="c-contact__th">受付時間</th>
                            <th class="c-contact__th">月</th>
                            <th class="c-contact__th">火</th>
                            <th class="c-contact__th">水</th>
                            <th class="c-contact__th">木</th>
                            <th class="c-contact__th">金</th>
                            <th class="c-contact__th">土</th>
                            <th class="c-contact__th">日・祝</th>
                        </tr>
                        <% if (typeof table !='undefined' ){ table.forEach(item=> { %>
                            <tr class="c-contact__tr">
                                <th class="c-contact__th">
                                    <%= item.th %>
                                </th>
                                <% item.td.forEach(i=> { %>
                                    <td class="c-contact__td">
                                        <i class="c-contact__status<% if(i.status == false){ %> is-off<% } else{ %> is-open<% } %>"></i>
                                    </td>
                                    
                            <% }) %>
                            </tr>
                                <% }) } %>
                    </tbody>
                </table>
                <ul class="c-contact__contact">
                    <li class="c-contact__item">
                        <span class="c-contact__titleitem">TEL </span><span class="c-contact__txtitem">092-407-5555</span>
                    </li>
                    <li class="c-contact__item">
                        <span class="c-contact__titleitem">FAX </span><span class="c-contact__txtitem">092-834-6900</span>
                    </li>
                    <li class="c-contact__item c-contact__address">
                        <span class="c-contact__titleitem">住所</span><span class="c-contact__txtitem">〒814-0133 福岡県福岡市城南区七隈1-18-21</span>
                    </li>
                </ul>
                <a href="#" class="c-contact__btn c-btn">交通アクセス</a>
            </div>
        </div>
        </div>
    </div>
    <div class="c-contact__map">
        <iframe class="c-contact__iframe"
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3324.5593436905965!2d130.3558500155739!3d33.56482365070458!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x354193850f438d1b%3A0x6099c48de5de226a!2s1-ch%C5%8Dme-18-21%20Nanakuma%2C%20Jonan%20Ward%2C%20Fukuoka%2C%20814-0133%2C%20Japan!5e0!3m2!1sen!2s!4v1672222049456!5m2!1sen!2s"
            allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    </div>
</section>